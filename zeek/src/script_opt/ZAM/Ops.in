# Branch on the value of v1 using switch table v2, with default branch to v3

macro EvalSwitchBody(cases, postscript)
	{
	auto t = cases[z.v2];
	if ( t.find(v) == t.end() )
		pc = z.v3;
	else
		pc = t[v];
	postscript
	continue;
	}

