unary-expr-op Cast
op-type X
set-type $$
set-type2 $1
eval	EvalCast(frame[z.v2].ToVal(z.t2))

macro EvalCast(rhs)
	std::string error;
	auto res = cast_value(rhs, z.t, error);
	if ( res )
		AssignV1(BuildVal(res, z.t))
	else
		ZAM_run_time_error(z.loc, error.c_str());

# Cast an "any" type to the given type.  Only needed for type-based swith
